#!/bin/bash
s=''
for x in `git branch -r --merged origin/master`; do
  if [[ "$x" == origin/releases/* ]]; then
    continue;
  fi
  if [ "$x" == origin/master ]; then
    continue;
  fi
  c=`git l -1 --since 4.week $x`;
  if [ -z "$c" ]; then
    s="$s :${x#origin/}"
  fi
done
[ -n "$s" ] && git push origin $s
